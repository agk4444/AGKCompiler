import math
import string
import list
import io

define function test_libraries:
    create sqrt_result as Float
    set sqrt_result to math.sqrt(16.0)
    io.println("Square root of 16 is: " + sqrt_result)

    create test_text as String
    set test_text to "Hello World"
    create upper_text as String
    set upper_text to string.uppercase(test_text)
    io.println("Uppercase: " + upper_text)

    create my_list as List
    set my_list to list.create_list()
    set my_list to list.append(my_list, "item1")
    set my_list to list.append(my_list, "item2")
    create list_length as Integer
    set list_length to list.length(my_list)
    io.println("List length: " + list_length)

    io.print("Enter your name: ")
    create user_name as String
    set user_name to io.read_line()
    io.println("Hello, " + user_name + "!")

    return "Library test completed"