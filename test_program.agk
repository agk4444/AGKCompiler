define function calculate_total that takes items:
    create total as Integer
    set total to 0
    return total

define function greet that takes name as String and returns String:
    return "Hello, " + name + "!"

define function fibonacci that takes n as Integer and returns Integer:
    if n is less than 2:
        return n
    else:
        return fibonacci(n - 1) + fibonacci(n - 2)